{
  "name": "react-tree-vis",
  "version": "1.0.0",
  "description": "React interactive tree visualization library.",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "publish-npm": "yarn build && yarn publish",
    "build": "rollup -c",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "deploy-storybook": "gh-pages -d storybook-static",
    "lint": "eslint 'src/**/*.{js,ts,tsx}' --quiet --fix",
    "format": "prettier --write \"**/*.{js,ts,tsx}\"",
    "format-check": "prettier --check \"**/*.{js,ts,tsx}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "type-check": "tsc --pretty --noEmit"
  },
  "pre-commit": [
    "lint",
    "format-check",
    "type-check",
    "test"
  ],
  "author": "Vandan Rogheliya",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/VandanRogheliya/react-tree-vis"
  },
  "homepage": "http://vandanrogheliya.github.io/react-tree-vis",
  "keywords": [
    "react",
    "hooks",
    "tree",
    "BST",
    "binary-search-tree",
    "binary tree",
    "visualization",
    "data-visualization",
    "interactive tree",
    "interactive",
    "insert",
    "search",
    "remove",
    "node"
  ],
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.2",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-docs": "^6.3.12",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/addon-storysource": "^6.2.9",
    "@storybook/react": "^6.2.9",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/react-hooks": "^7.0.0",
    "@types/jest": "^26.0.23",
    "@types/mdx": "^2.0.1",
    "@types/node": "^15.6.1",
    "@types/testing-library__jest-dom": "^5.9.5",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "babel-loader": "^8.2.2",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gh-pages": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.4.2",
    "postcss": "^8.3.0",
    "pre-commit": "^1.2.2",
    "prettier": "^2.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rollup": "^2.48.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-jest": "^26.4.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {}
}
